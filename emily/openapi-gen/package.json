{
  "name": "emily-api",
  "scripts": {
    "build-testing-api-rust": "JAVA_OPTS='-Dlog.level=error' openapi-generator-cli generate -i ./generated-specs/testing-emily-openapi-spec.json -g rust -o $EMILY_CLIENTS_DIR/rust/testing/ --config ./api-config/testing.json",
    "build-private-api-rust": "JAVA_OPTS='-Dlog.level=error' openapi-generator-cli generate -i ./generated-specs/private-emily-openapi-spec.json -g rust -o $EMILY_CLIENTS_DIR/rust/private/ --config ./api-config/private.json",
    "build-public-api-rust": "JAVA_OPTS='-Dlog.level=error' openapi-generator-cli generate -i ./generated-specs/public-emily-openapi-spec.json -g rust -o $EMILY_CLIENTS_DIR/rust/public/ --config ./api-config/public.json",
    "build-private-api-python": "JAVA_OPTS='-Dlog.level=error' openapi-generator-cli generate -i ./generated-specs/private-emily-openapi-spec.json -g python -o $EMILY_CLIENTS_DIR/python/private/ --config ./api-config/private.json --package-name emily_client",
    "build": "pnpm build-testing-api-rust && pnpm build-public-api-rust && pnpm build-private-api-rust && pnpm build-private-api-python"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.2"
  }
}